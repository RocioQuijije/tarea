public class ejemplo2 {
    public static void main(String[] args) {
     
  float a, b, c;
            Random rnd = new Random();
            a = (rnd.nextFloat() * 10);
            b = (rnd.nextFloat() * 10);
            c = (rnd.nextFloat() * 10);
            System.out.println(a);        
            System.out.println(b);        
            System.out.println(c);        
      }
} 
